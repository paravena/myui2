    getMessage : (messageId, options) ->
        options = options || {}
        result = messageId
        language = window.navigator.userLanguage || window.navigator.language
        languageCd = language.substring(0,2).toUpperCase()
        try
            messages = @[languageCd] || @['EN']
            if messages[messageId]
                temp = messages[messageId];
                for p of options
                    re = new RegExp('#\{'+p+'\}','g')
                    temp = temp.replace(re, options[p])
                return temp
        catch e
            result = messageId;
        return result
